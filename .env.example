# === MySQL 設定 ===
DB_HOST=localhost        # Docker未使用なら localhost, docker-compose なら "mysql"
DB_PORT=3306
DB_USER=root
DB_PASSWORD=example
DB_NAME=typescript_batch

# === RabbitMQ 設定 ===
RABBITMQ_HOST=localhost  # docker-composeなら "rabbitmq"
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_QUEUE_IN=create_user
RABBITMQ_QUEUE_OUT=user_created

# === その他（任意） ===
LOG_LEVEL=info
